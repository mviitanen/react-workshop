{
  "name": "react-training-workshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "app": "yarn start app",
    "test": "NODE_PATH=./apps jest",
    "clean": "git clean -fdX .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "kill-db-port": "node scripts/kill-db-port 3333",
    "create-db": "node scripts/create-db",
    "postinstall": "yarn create-db"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/classnames": "^2.2.10",
    "@types/form-serialize": "^0.7.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "find-process": "^1.4.3",
    "jest": "25.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "prettier": "^2.0.1"
  },
  "dependencies": {
    "@babel/core": "7.8.7",
    "@reach/descendants": "^0.9.1",
    "@reach/menu-button": "^0.9.1",
    "@reach/rect": "^0.9.0",
    "@reach/tabs": "^0.9.1",
    "@svgr/webpack": "5.2.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.1",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "classnames": "^2.2.6",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "file-loader": "6.0.0",
    "form-serialize": "^0.7.2",
    "fs-extra": "^8.1.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "json-server": "^0.16.1",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.0",
    "react-dom": "^16.13.0",
    "react-flex-columns": "^0.4.3",
    "react-icons": "^3.9.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "readline-sync": "^1.4.10",
    "resolve": "1.15.1",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "6.3.0",
    "shelljs": "^0.8.3",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-pnp": "1.1.6",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/rules-of-hooks": 2,
      "react-hooks/exhaustive-deps": 1,
      "@typescript-eslint/no-unused-vars": 0
    },
    "overrides": [
      {
        "files": [
          "apps/**/*.tsx",
          "courses/**/*.final.tsx"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": 1
        }
      },
      {
        "files": [
          "**/*.js"
        ],
        "rules": {
          "no-unused-vars": 0
        }
      },
      {
        "files": [
          "apps/**/*.js",
          "courses/**/*.final.js"
        ],
        "rules": {
          "no-unused-vars": 1
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
